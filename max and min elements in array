#include <stdio.h>  // max and min element in array by bharat dhiman
int main()
{
    int size, a[100], i;
    scanf("%d",&size);
    for(i=0;i<size;i++)
    {
        scanf("%d",&a[i]);
    }
    int min = a[0];
    int max = a[0];
    for(i=0;i<size;i++)
    {
        if(max<a[i])
        {
            max = a[i];
        }
        if(min>a[i])
        {
            min = a[i];        
        }
    }
    printf("%d\n", min);
    printf("%d", max);

    return 0;
}
